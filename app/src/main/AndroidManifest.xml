<!--					android:scheme="msauth" />-->
<!--			</intent-filter>-->
<!--		</activity>-->
		<activity android:name=".InitActivity">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity> <!-- android:label="MainActivity" -->
		<!-- android:theme="@style/Theme.AppCompat" -->
		<activity
			android:name=".MainActivity"
			android:launchMode="singleTop">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
			<meta-data
				android:name="android.app.searchable"
				android:resource="@xml/searchable_sharename" />
		</activity>
		<activity android:name=".SharenameSearchableActivity">
			<intent-filter>
				<action android:name="android.intent.action.SEARCH" />
			</intent-filter>
			<meta-data
				android:name="android.app.searchable"
				android:resource="@xml/searchable_sharename" />
		</activity>
		<activity
			android:name=".PlayMedia"
			android:parentActivityName=".MainActivity"
			tools:targetApi="jelly_bean">
			<meta-data
				android:name="android.support.PARENT_ACTIVITY"
				android:value=".MainActivity" />
		</activity>
		<activity android:name=".Helper.showFTPsettingsActivity" /> <!-- android:theme="@style/PreferencesTheme" -->
		<activity
			android:name=".Settings.SettingsActivity"
			android:label="@string/settings"
			android:parentActivityName=".MainActivity"
			tools:ignore="InnerclassSeparator"
			tools:targetApi="jelly_bean">
			<intent-filter>
				<action android:name="android.intent.action.MANAGE_NETWORK_USAGE" />
				<category android:name="android.intent.category.DEFAULT" />
			</intent-filter>
			<meta-data
				android:name="android.support.PARENT_ACTIVITY"
				android:value=".MainActivity" />
		</activity>
		<activity
			android:name="com.dropbox.core.android.AuthActivity"
			android:configChanges="orientation|keyboard"
			android:launchMode="singleTask">
			<intent-filter>
				<data android:scheme="db-0qb7zuth7zcx40s" />
				<action android:name="android.intent.action.VIEW" />
				<category android:name="android.intent.category.BROWSABLE" />
				<category android:name="android.intent.category.DEFAULT" />
			</intent-filter>
		</activity>
		<activity android:name=".Adapter.LeftArrayAdapter" />

		<service
			android:name=".Service.MyService"
			android:enabled="true"
			tools:ignore="InnerclassSeparator" />
