<!--				&lt;!&ndash; <data android:scheme="msal8cd4df27-a31a-4ede-a3b0-f1f7408ac8f5" &ndash;&gt;-->
<!--				&lt;!&ndash; android:host="auth" /> &ndash;&gt;-->
